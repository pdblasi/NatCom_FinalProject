unset key
set xrange [0:500]
set yrange [0:500]
set term png

set output "24900state.png"
plot "0state24900.dat" using 1:2 title "Player 1", \
     "1state24900.dat" using 1:2 title "Player 2", \
     "2state24900.dat" using 1:2 title "Player 3", \
     "3state24900.dat" using 1:2 title "Player 4"

replot
